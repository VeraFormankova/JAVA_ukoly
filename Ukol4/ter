[32m+[m[32m        this.kapacita = kapacita;[m
[32m+[m[32m        this.typObsahu = typObsahu;[m
[32m+[m[32m        this.stav = 0; // Po vytvo≈ôen√≠ je n√°dr≈æ pr√°zdn√°[m
[32m+[m[32m    }[m
